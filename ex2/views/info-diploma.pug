extends layout

block content
    .w3-container
        .w3-card-4
            header.w3-container.w3-teal.w3-margin-top
                h3= item.sumario
            table.w3-table-all
                tr
                    th Data
                        td= item.data
                tr
                    th Sumário
                        td= item.sumario
                tr
                    th Estado
                        td= item.estado
                tr
                    th Link
                        td= item.link  
                tr
                    th Entidades Responsáveis
                        td
                           a(href="http://clav-api.dglab.gov.pt/api/entidades/" + item.entidade + "?apikey=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpYXQiOjE1Nzg4NjAwNTQsImV4cCI6MTU4MTQ1MjA1NH0.HIlH4_Ao6504qaLhhbZ2_OtDzaZaG5FeYy-Yc2d9lwQ")=item.entidade


        
        .w3-card-4
            header.w3-container.w3-teal.w3-margin-top
                h3 Lista de processos que regula
            table.w3-table-all
                tr
                    th Identificador
                    th Codigo
                    th Titulo
                each p in proc
                    tr
                        td
                            a(href="http://clav-api.dglab.gov.pt/api/classes/" + p.id + "?apikey=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpYXQiOjE1Nzg4NjAwNTQsImV4cCI6MTU4MTQ1MjA1NH0.HIlH4_Ao6504qaLhhbZ2_OtDzaZaG5FeYy-Yc2d9lwQ")= p.id
                        td= p.codigo
                        td= p.titulo
        
    footer.w3-container.w3-teal Made by Shahzod Yusupov
        a(href='/')
            button.w3-button.w3-green.w3-margin Voltar
